pipeline { // Declarative pipelineであることを宣言する
  agent any
  parameters {
        // ticket no
        string(
            name: 'text',
            defaultValue: 'x-japan',
            description: ''
        )
    }
    stage('git checkout') {
        steps {
          sh """
            git pull origin master
            git checkout master
          """
    }
    stage('find files') {
        steps {
          copyArtifacts fingerprintArtifacts: true, projectName: 'dev-myRepository'
        }
    }
    stage('push') {
        steps {
          sh """
            git pull origin master
            git checkout master
            git add .
            git config --global user.email 'chiru.5emon@gmail.com'
            git config --global user.name 'chiruchiruchiru'
            git commit -m 'commit'
            git push origin master
          """
        }
    }
  }
}