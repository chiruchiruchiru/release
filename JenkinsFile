pipeline { // Declarative pipelineであることを宣言する
  agent any
  parameters {
        // ticket no
        string(
            name: 'text',
            defaultValue: 'x-japan',
            description: ''
        )
    }
  stages {
    stage('find files') {
        steps {
          copyArtifacts fingerprintArtifacts: true, projectName: 'dev-myRepository'
        }
    }
    stage('push') {
        steps {
          sh """
            git add .
            git commit -m "commit"
            git push origin/master
          """
        }
    }
  }
}