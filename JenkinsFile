pipeline { // Declarative pipelineであることを宣言する
  agent any
  parameters {
        // ticket no
        string(
            name: 'text',
            defaultValue: 'x-japan',
            description: ''
        )
    }
  stages {
    stage('find files') {
        steps {
          copyArtifacts fingerprintArtifacts: true, projectName: 'dev-myRepository'
        }
    }
  }
}